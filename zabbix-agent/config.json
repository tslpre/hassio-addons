{
    "name": "Zabbix Agent",
    "url": "https://git.asperti.com/paspo/hassio-addons",
    "version": "0.1.2",
    "slug": "zabbix-agent",
    "description": "Zabbix Agent for hass.io",
    "startup": "system",
    "boot": "auto",
    "full_access": true,
    "host_ipc": true,
    "host_pid": true,
    "host_network": true,
    "docker_api": true,
    "audio": false,
    "gpio": false,
    "udev": true,
    "devices": [
        "/dev/vchiq:/dev/vchiq:rwm",
        "/dev/vhci:/dev/vhci:rwm"
    ],
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "i386",
        "armv7"
    ],
    "ports": {
        "10050/tcp": 10050
    },
    "map": [
        "share"
    ],
    "options": {
        "server": "zabbix-server",
        "hostname": "hassio"
    },
    "schema": {
        "server": "str",
        "hostname": "str"
    }
}
